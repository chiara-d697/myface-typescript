<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ul>
    <% results.forEach(function(user) {%>
        <li>
            Name: <%= user.name %>
        </li>
        <li>
            Username: <%= user.username %>
        </li>
        <li>
            <a href="/users/<%=user.id%>">
            <img src="<%=user.profileImageUrl%>" alt="User profile image" />
            </a>
        </li>
    <% }); %>
</ul>
<%if (next) { %>
    <a href="<%= next %>">Next</a>
<% } %>
<%if (previous) { %>
    <a href="<%= previous %>">Previous</a>
<% } %>
</body>
<%- include('_footer') -%>
</html>